{
  "name": "fypai-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Collaborative AI-enabled productivity app for students/teams",
  "workspaces": [
    "packages/*",
    "frontend",
    "backend"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:types": "npm run build --workspace=@fypai/types",
    "build:backend": "npm run build --workspace=backend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:all": "npm run build:types && npm run build:backend && npm run build:frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:types": "npm run dev --workspace=@fypai/types",
    "test:backend": "npm run test --workspace=backend",
    "test:frontend": "npm run test --workspace=frontend",
    "clean": "rm -rf node_modules packages/*/node_modules frontend/node_modules backend/node_modules packages/*/dist backend/dist frontend/dist"
  },
  "keywords": [
    "collaborative",
    "ai",
    "productivity",
    "chat",
    "teams"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
